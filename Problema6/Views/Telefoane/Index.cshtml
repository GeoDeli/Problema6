@model IEnumerable<Problema6.Models.Telefon>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")

    @using (Html.BeginForm())
    {
    <p>
        Model telefon: @Html.TextBox("Model") <br />
        <input type="submit" value="Cauta" />
    </p>
}
    </p>
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Model)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.An)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Specificatii)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Pret)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Stoc)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Model)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.An)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Specificatii)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Pret)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Stoc)
                </td>
                <td>
                    @Html.ActionLink("Edit", "Edit", new { id = item.ID_telefon }) |
                    @Html.ActionLink("Details", "Details", new { id = item.ID_telefon }) |
                    @Html.ActionLink("Delete", "Delete", new { id = item.ID_telefon })
                </td>
            </tr>
        }

    </table>
